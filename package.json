{
  "name": "multicolour",
  "version": "0.6.45",
  "description": "Generate your REST API with JSON and use almost any DB tech.",
  "keywords": [
    "Multicolour",
    "REST API",
    "generator",
    "ORM"
  ],
  "main": "index.js",
  "preferGlobal": true,
  "bin": {
    "multicolour": "cli.js",
    "multicolor": "cli.js",
    "color": "cli.js"
  },
  "scripts": {
    "linter": "eslint ./**/*.js",
    "unit-tests": "tape tests/all.js | tap-spec",
    "test": "npm run linter && npm run unit-tests",
    "test-coverage": "istanbul cover tape tests/*.js --recursive --report lcov -x '**/cli/**' -x '**/waterline-fixes/**' | tap-spec",
    "changelog": "github_changelog_generator -u Multicolour -p multicolour"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Multicolour/multicolour.git"
  },
  "author": "newworldcode <hello@newworld.codes> (https://newworld.codes)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Multicolour/multicolour/issues"
  },
  "homepage": "https://github.com/Multicolour/multicolour#readme",
  "dependencies": {
    "@newworldcode/talkie": "^1.0.9",
    "async": "^1.5.2",
    "chalk": "^2.3.0",
    "cli-spinner": "^0.2.7",
    "commander": "^2.11.0",
    "cross-spawn": "^5.1.0",
    "debug": "^3.1.0",
    "fs-extra": "^4.0.2",
    "inquirer": "^4.0.0",
    "joi": "^13.0.2",
    "jsocrud": "^2.0.3",
    "mkdirp": "^0.5.1",
    "npm-registry-client": "^8.5.0",
    "pluralize": "^7.0.0",
    "process-nextick-args": "^1.0.7",
    "replacestream": "^4.0.3",
    "uuid": "^3.1.0",
    "validate-npm-package-name": "^3.0.0",
    "waterline": "0.12",
    "waterline-joi": "^1.1.5"
  },
  "engine-strict": true,
  "engineStrict": true,
  "engines": {
    "node": ">= 4.5.0"
  },
  "devDependencies": {
    "blue-tape": "^1.0.0",
    "eslint": "^4.11.0",
    "istanbul": "^0.4.5",
    "newman": "^3.8.3",
    "sails-memory": "^0.10.7",
    "snyk": "^1.49.5",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0"
  }
}
